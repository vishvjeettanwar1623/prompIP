# Wallet Configuration (without 0x prefix)
WALLET_PRIVATE_KEY=your_private_key_without_0x_prefix

# Story Protocol RPC
RPC_PROVIDER_URL=https://aeneid.storyrpc.io

# JWT Secret
JWT_SECRET=your_super_secret_jwt_key_change_this

# Database (Use a cloud database like Neon, PlanetScale, or Supabase for production)
DATABASE_URL=postgresql://user:password@localhost:5432/prompip?schema=public

# Server
PORT=3001

# Frontend URL (for CORS in production)
FRONTEND_URL=https://your-frontend.vercel.app

# Story Protocol Configuration (Optional - SDK uses defaults if not set)
# NFT_CONTRACT_ADDRESS=0x...
# LICENSE_TEMPLATE_ADDRESS=0x...
